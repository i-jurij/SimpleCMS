////////////////
tables:
table 'clients' - id, name, contacts (phone, messenger, social etc)
table 'orders' - id, client_id (clients.id), services (services.id), master (masters.id), price (services.price), order_datetime, completed_datetime
table 'services_categories' -
            "id" => [
                "INTEGER PRIMARY KEY",
                "AUTOINCREMENT",
                "NOT NULL"
            ],
            "page_id" => [
                "INTEGER",
                "NOT NULL"
            ],
            "category_img" => [
                "TEXT"
            ],
            "category_name" => [
                "TEXT"
            ],
            "category_descr" => [
                "TEXT"
            ],
            "FOREIGN KEY (page_id) REFERENCES pages(id) ON DELETE CASCADE ON UPDATE CASCADE"

table 'services' -
            "id" => [
                "INTEGER PRIMARY KEY",
                "AUTOINCREMENT",
                "NOT NULL"
            ],
            "page_id" => [
                "INTEGER",
                "NOT NULL"
            ],
            "category_id" => [
                "INTEGER"
            ],
            "service_img" => [
                "TEXT"
            ],
            "service_name" => [
                "TEXT"
            ],
            "service_descr" => [
                "TEXT"
            ],
            "price" => [
                "REAL"
            ],
            "duration" => [
                "INTEGER"
            ],
            "FOREIGN KEY (page_id) REFERENCES pages(page_id) ON DELETE CASCADE ON UPDATE CASCADE"
table 'callback' - id, client_id (clients.id), orders (orders.id), send, call_datetime, response_datetime, response_yes (yes or no, or bool)

admin user pages:
1. need call back +
2. call back log +

admin moder page:
1. clear call back log +

client page:
1. callback
